<h2>Here are all the albums:</h2>

<ul>
  <% @artists.cd().each() do |artist| %>
    <li><a href="/artist/<%= artist.id() %>"><%= artist.artist_name() %> <%= artist.songs() %> <%= artist.gene() %></a></li>
  <% end %>
</ul>

<a href="/artist/<%= @artists.id() %>/cd/new"><button type="button">Add a new Album</button></a>
